{
  "name": "@vscode-use/utils",
  "type": "module",
  "version": "0.1.58",
  "packageManager": "pnpm@10.12.1",
  "description": "A collection of utility functions for VS Code extensions. Enhance your VS Code extension development with reusable, well-tested utilities.",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/vscode-use/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vscode-use/utils.git"
  },
  "bugs": {
    "url": "https://github.com/vscode-use/utils/issues"
  },
  "keywords": [
    "vscode-use",
    "@vscode-use/utils",
    "utils",
    "vscode",
    "vscode-extension",
    "vscode utilities",
    "typescript",
    "node",
    "productivity",
    "developer-tools"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./index": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.92.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsdown src/index.ts",
    "dev": "tsdown --watch src",
    "format": "prettier --write --cache .",
    "lint": "eslint . --cache",
    "lint:fix": "nr lint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "alien-signals": "^1.0.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@antfu/ni": "^0.21.12",
    "@types/node": "^18.19.86",
    "@types/vscode": "1.92.0",
    "bumpp": "^8.2.1",
    "eslint": "^9.25.1",
    "picocolors": "^1.1.1",
    "prettier": "^2.8.8",
    "tsdown": "^0.9.6",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint . --fix"
  }
}
